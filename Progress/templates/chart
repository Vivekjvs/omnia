      <script>
            var canvas = document.getElementById("myChart");
            var ctx = canvas.getContext('2d');

            var data = {
                labels: [{% for item in labels %}
                 "{{item|safe}}",
                 {% endfor %}],
                datasets: [
                    {
                        fill: true,
                        backgroundColor: [
                        "rgb(76,175,80)",
                            'Red',
                            'Blue',
                            'Orange',
                        'Purple'],
                        hoverOffset : 4,
                       
                        data: [{% for item in values[0] %}
                            {{item|safe}},
                            {% endfor %}],
            // Notice the borderColor 
                        borderColor:	['black', 'black'],
                        borderWidth: [2,2]
                    }
                ]
            };

            // Notice the rotation from the documentation.
            text = 'Your contest Performance';
            var options = {
                    title: {
                            display: true,
                            text: 'Your Contest Performance',
                            position: 'top'
                        },
                        responsive : false
            };


            // Chart declaration:
            var myPieChart = new Chart(ctx, {
                type: 'doughnut',
                data: data,
                options: options
            });
      </script>
      <script>
            var canvass = document.getElementById("graphChart");
            var context = canvass.getContext('2d');

            const glabels = [{% for item in lab %}
                 "{{item[0]|safe}}",
                 {% endfor %}];
                const gdata = {
                labels: glabels,
                datasets: [{
                    label:'Your growth',
                    //backgroundColor: 'rgb(255, 99, 132)',
                    borderColor: 'rgb(255, 99, 132)',
                    data: [{% for item in val %}
                            {{item[0]|safe}},
                            {% endfor %}],
                }]
            };

            const configuration = {
                type: 'line',
                data: gdata,
                options: {
                  scales: {
                            yAxes: [
                            {
                                stacked: true,
                                gridLines: {
                                display: true,
                                color: "rgba(255,99,132,0.2)"
                                },
                                ticks: {
                                suggestedMax: 100,
                                suggestedMin: -10
                                }
                            }
                            ],
                            xAxes: [
                            {
                                gridLines: {
                                display: false
                                }
                            }
                            ]
                        }},
                        responsive : false,
                        
            };
            
            const chart = new Chart(
                document.getElementById('graphChart'),
                configuration
            );
      </script>
        
   
